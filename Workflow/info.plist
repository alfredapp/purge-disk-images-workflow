				<key>script</key>
				<string>#!/usr/bin/osascript -l JavaScript

function envVar(varName) {
  return $.NSProcessInfo
    .processInfo
    .environment
    .objectForKey(varName).js
}

function run() {
  // Get mounted Disk Images
  const task = $.NSTask.alloc.init
  const stdout = $.NSPipe.pipe

  task.executableURL = $.NSURL.fileURLWithPath("/usr/bin/hdiutil")
  task.arguments = ["info", "-plist"]
  task.standardOutput = stdout
  task.launchAndReturnError(false)

  const dataOut = stdout.fileHandleForReading.readDataToEndOfFileAndReturnError(false)
  const stringOut = $.NSString.alloc.initWithDataEncoding(dataOut, $.NSUTF8StringEncoding).js

  // Use temporary plist to parse info as JSON
  const tmpPlist = `/tmp/${envVar("alfred_workflow_bundleid")}.plist`
  $(stringOut).writeToFileAtomicallyEncodingError(tmpPlist, true, $.NSUTF8StringEncoding, undefined)
  const diskImages = Application("System Events")
    .propertyListFiles.byName(tmpPlist).contents.value().images
    .filter(image =&gt; !image["image-path"].startsWith("/Library"))
    .filter(image =&gt; !image["image-path"].startsWith("/System"))

  // When no disk images found
  if (diskImages.length === 0) return JSON.stringify({ items: [{
    title: "No Disk Images Mounted",
    valid: false
  }]})

  // Show disk images
  const sfItems = diskImages.flatMap(diskImage =&gt; {
    const lastItem = diskImages.length === 1 // When trashing last item, hide Alfred
    const originPath = diskImage["image-path"]
    const mountPoint = diskImage["system-entities"]
      .find(entity =&gt; entity["mount-point"])["mount-point"]

    return {
      variables: {
        last_item: lastItem,
        mount_point: mountPoint,
        origin_path: originPath
      },
      title: $(mountPoint).lastPathComponent.js,
      subtitle: `Eject ${mountPoint} and trash ${$(originPath).stringByAbbreviatingWithTildeInPath.js}`,
      arg: "See variables",
      icon: { path: mountPoint, type: "fileicon" }
    }
  })

  return JSON.stringify({ items: sfItems })
}</string>
